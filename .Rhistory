library(cranlogs)
library(dplyr)
library(lubridate)
test <- cran_downloads(from = "2018-02-05", to = "2020-02-14", packages = "CENFA")
df <- test %>%
group_by(month = floor_date(date, "month")) %>%
#group_by(month, year) %>%
summarise(total = sum(count))
plot(df$month, cumsum(df$total), type = "b")
plot(df$month, df$total, type = "b", ylim = c(0, 500))
mean(df$total)
sum(df$total)
